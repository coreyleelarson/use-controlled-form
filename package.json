{
  "name": "use-controlled-form",
  "version": "0.0.14",
  "author": "Corey Larson <coreyleelarson@gmail.com>",
  "description": "Create simple controlled React forms, using hooks, without all the fluff!",
  "keywords": [
    "controlled",
    "form",
    "forms",
    "react",
    "hook",
    "hooks"
  ],
  "homepage": "https://github.com/coreyleelarson/use-controlled-form",
  "repository": {
    "type": "git",
    "url": "https://github.com/coreyleelarson/use-controlled-form.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack",
    "lint": "eslint '**/*.js'",
    "prepublish": "npm run build",
    "test": "jest --config .jestrc.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "lint-staged": "^8.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-testing-library": "^5.9.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "react": ">=16.8"
  }
}
